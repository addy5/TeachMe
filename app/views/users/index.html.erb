<div id="coverPic">
  <p class="bfQuote">
    "Tell me and I forget.</br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Teach me and I remember."</br>  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;- <span id="bfSign"> Ben Franklin </span>
  </p>
</div>

<h1 class='title'>TeachMe:</h1>

<% @posts.each do |p|%>

  <div class="indexListing">
    <h6><%=p.title.capitalize%></h6>
    <img src="<%=p.profile_url %>" alt="" />
    <p>With "Mr." <%= p.user.first_name%></p>
    <p>In: <%= p.city%></p>
  </div>

<% end %>
